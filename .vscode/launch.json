{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "todoList",
      "program": "${workspaceFolder}/examples/todoList/index.js"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/examples/api/process/stdin.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server-client-structure",
      "program": "${workspaceRoot}/mini-programs/server-client-structure/server/index.js"
    },
    {
      "name": "web-spider",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/web-spider/www.duotoo.com/index.js",
      "stopOnEntry": false,
      "args": ["waiguomeinv/33601"],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": ["--nolazy"],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": false
    }
  ]
}
